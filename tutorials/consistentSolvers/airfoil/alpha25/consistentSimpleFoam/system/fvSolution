FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          PCG;
        tolerance       1e-7;
        relTol          0.1;
        preconditioner  DIC;
        maxIter		500;
    }

    /*p
    {
        solver           GAMG;
        tolerance        1e-7;
        relTol           0.1;
        smoother         GaussSeidel;
        nPreSweeps       0;
        nPostSweeps      2;
        cacheAgglomeration on;
        agglomerator     faceAreaPair;
        nCellsInCoarsestLevel 68;//sqrt(70000) = 270, 270/4 = 68
        mergeLevels      1;
    };*/

    "(U|k|omega)"
    {
        solver          smoothSolver;
        smoother  GaussSeidel;
        tolerance       1e-7;
        relTol          0.1;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
    pRefPoint      (-12 0 0);
    pRefValue       0;

    convergence 1e-5;
}

relaxationFactors
{
    p               	0.7;
    U               	0.7;
    k         		0.7;
    omega      		0.7;
}


