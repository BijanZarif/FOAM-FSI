FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-10;
        relTol          0.05;
        maxIters        400;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 30;
    pRefPoint      (-12 0 0);
    pRefValue       0;
}
