FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    Up
    {
        solver BiCGStab;
        preconditioner Cholesky;

        tolerance 1e-11;
        relTol 1.0e-3;

        minIter 1;
        maxIter 500;
    }

    p
    {
      solver           PCG;
      preconditioner   DIC;
      tolerance        1e-11;
      relTol           1.0e-3;
    }

    U
    {
        solver          BiCGStab;
        preconditioner  DILU;
        tolerance       1e-11;
        relTol          1.0e-3;
    }
}

PIMPLE
{
  nOuterCorrectors 500;
  nCorrectors 5;
  nNonOrthogonalCorrectors 0;
}

blockSolver
{
    pRefCell 0;
    pRefValue 0;
    nOuterCorrectors 500;
    convergenceTolerance 1e-9;
}


