#!/bin/bash

set -e

export LIBLAPACK="-llapack -lblas"

wmakeLnInclude src/RBFmeshInterpolation
wmakeLnInclude src/fvPatchFields
wmakeLnInclude src/fvSchemes
wmakeLnInclude src/fsi
wmakeLnInclude src/IDWmeshDeformation

wmake libso src/RBFmeshInterpolation
wmake libso src/fvPatchFields
wmake libso src/fvSchemes
wmake libso src/fsi
wmake libso src/IDWmeshDeformation
wmake applications/solvers/unsteadyPUDyMCoupledFoam
wmake applications/solvers/unsteadyPUCoupledFoam
wmake applications/solvers/fsi/fsiFoam
